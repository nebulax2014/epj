	<!-- Header area -->
	<!-- Header area -->

	<!-- Navigation & Logo-->
	<% include components/menu.ejs %>

	<!-- Homepage Slider -->
	<div class="homepage-slider">
		<div id="sequence">
			<ul class="sequence-canvas">
				<!-- Slide 1 -->
				<li class="bg4">
					<!-- Slide Title -->
					<h2 class="title">Responsive</h2>
					<!-- Slide Text -->
					<h3 class="subtitle">It looks great on desktops, laptops, tablets and smartphones</h3>
					<!-- Slide Image -->
					<img class="slide-img" src="images/homepage-slider/slide1.png" alt="Slide 1" />
				</li>
				<!-- End Slide 1 -->
				<!-- Slide 2 -->
				<li class="bg3">
					<!-- Slide Title -->
					<h2 class="title">Color Schemes</h2>
					<!-- Slide Text -->
					<h3 class="subtitle">Comes with 5 color schemes and it's easy to make your own!</h3>
					<!-- Slide Image -->
					<img class="slide-img" src="images/homepage-slider/slide2.png" alt="Slide 2" />
				</li>
				<!-- End Slide 2 -->
				<!-- Slide 3 -->
				<li class="bg1">
					<!-- Slide Title -->
					<h2 class="title">Feature Rich</h2>
					<!-- Slide Text -->
					<h3 class="subtitle">Huge amount of components and over 30 sample pages!</h3>
					<!-- Slide Image -->
					<img class="slide-img" src="images/homepage-slider/slide3.png" alt="Slide 3" />
				</li>
				<!-- End Slide 3 -->
			</ul>
			<div class="sequence-pagination-wrapper">
				<ul class="sequence-pagination">
					<li>1</li>
					<li>2</li>
					<li>3</li>
				</ul>
			</div>
		</div>
	</div>
	<!-- End Homepage Slider -->

	<div class="container">
		<div id="searchContainer">
			<div class="row">
				<form action="/searchResult" method="get">
					<div class="col-xs-12">
						<div class="input-group">
							<input id="searchTxt" name="searchTxt" type="text" class="form-control">
							<div class="input-group-btn">
								<button type="submit" class="btn btn-default dropdown-toggle">搜索 </button>
								<!--<ul class="dropdown-menu dropdown-menu-right" role="menu">
                                    <li><a href="#">Action</a></li>
                                    <li><a href="#">Another action</a></li>
                                    <li><a href="#">Something else here</a></li>
                                    <li class="divider"></li>
                                    <li><a href="#">Separated link</a></li>
                                </ul>-->
							</div>
							<!-- /btn-group -->
						</div>
						<!-- /input-group -->
					</div>
					<!-- /.col-lg-6 -->
				</form>
			</div>
			<!-- /.row -->

		</div>
	</div>


	<section id="jQuery-plugin" class="portfolio-area">
		<div class="container">
			<h2 class="block_title">搜索结果</h2>
			<div id="searchContainer">
				<div class="row port cs-style-3">

					<% for(var i=0; i<items.length; i++){%>
						<div class="col-md-4 col-sm-6 col-xs-12 item-space">
							<figure>
								<a href="detail?id=<%= items[i]._id %>">
									<%= items[i].mc_name %>
								</a>

							</figure>
						</div>
						<% }%>
				</div>
				<div class="row port cs-style-3">

				</div>

			</div>

		</div>
	</section>



	<!-- Footer -->
	<% include components/footer.ejs %>

	<!-- Javascripts -->
	<script src="js/jquery-1.12.4.min.js"></script>
	<script src="js/bootstrap.min.js"></script>
	<script src="js/leaflet.js"></script>
	<script src="js/jquery.fitvids.js"></script>
	<script src="js/jquery.sequence-min.js"></script>
	<script src="js/jquery.bxslider.js"></script>
	<script src="js/main-menu.js"></script>
	<script src="js/template.js"></script>
	<script src="js/jquery-ui.js"></script>
	<script>
    var searchPath="";
    $( "#searchTxt" ).autocomplete({
        source: searchPath+"/search"
     });
    </script>


</body>

</html>